# Docker Compose Environment Variables
# Copy this file to .env and update with your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (for Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# AEM Configuration
AEM_HOST=http://localhost:4502
AEM_SERVICE_USER=admin
AEM_SERVICE_PASSWORD=admin

# MCP Server Configuration
MCP_USERNAME=admin
MCP_PASSWORD=admin

# Project Configuration
AEM_PROJECT_PATH=/app/projects

# GitHub Configuration (for EDS integration)
GITHUB_TOKEN=your_github_token_here

GIT_REPO_URL=https://github.com/your-username/your-repo.git
GIT_BASE_BRANCH=main
GIT_BRANCH_PREFIX=eds/
GIT_AUTHOR_NAME=DXP Bot
GIT_AUTHOR_EMAIL=dxp-bot@example.com
